class person:
    def __init__(self,name,age):
        self.name=name
        self.age=age
    def __str__(self):
        return f"{self.name},{self.age} years old"
    def __repr__(self):
        return f"persnname={self.name},age={self.age}"
person=person("rohith",19) 
print(person)
print(repr(person))
    