## insertion sort
def insertion_sort(arr):
    n=len(arr)
    for i in range(1,n):
        key=arr[i]
        j=i-1
        while j>=0 and arr[j]>key:
            arr[j+1]=arr[j]
            j-=1
        arr[j+1]=key
        print(f"after pass {i}: {arr}") 
    return arr
arr=[3,4,5,6]
sort=insertion_sort(arr)
print(sort)       